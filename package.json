{
  "name": "tech-radar-web",
  "version": "1.0.0",
  "description": "Tech Radar Weekly - Cyberpunk style tech newsletter",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "test": "node tests/run-tests.js",
    "test:unit": "node tests/run-tests.js unit",
    "test:integration": "node tests/run-tests.js integration",
    "test:e2e": "node tests/run-tests.js e2e",
    "test:perf": "node tests/performance/run-performance-tests.js",
    "test:perf:api": "artillery run tests/performance/scenarios/load-test.yml",
    "test:perf:load": "node tests/performance/scenarios/baseline.js",
    "test:perf:monitor": "node tests/performance/monitoring/performance-monitor.js",
    "test:perf:baseline": "node tests/performance/scenarios/baseline.js",
    "test:perf:integration": "node tests/performance/run-performance-tests.js --scenarios light,moderate"
  },
  "dependencies": {
    "chokidar": "^3.5.3",
    "express": "^4.18.2",
    "typescript": "^5.9.3",
    "typescript-language-server": "^5.1.3"
  },
  "devDependencies": {
    "@fontsource/inter": "^5.2.8",
    "@fontsource/jetbrains-mono": "^5.2.8",
    "artillery": "^2.0.28",
    "autocannon": "^8.0.0",
    "esbuild": "^0.27.2",
    "highlight.js": "^11.9.0",
    "js-yaml": "^4.1.0",
    "marked": "^11.1.1"
  }
}
